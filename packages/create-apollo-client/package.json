{
  "name": "@8base/create-apollo-client",
  "version": "0.1.3",
  "main": "es/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src -d es --ignore __tests__",
    "doc": "NODE_ENV=test documentation readme",
    "docs": "yarn doc src/createApolloClient.js --section=createApolloClient && yarn doc src/createApolloLinks/createApolloLinks.js --section=createApolloLinks",
    "test": "NPM_ENV=test jest",
    "watch": "NODE_ENV=development babel src -d es --watch --ignore __tests__"
  },
  "devDependencies": {
    "apollo-test-utils": "^0.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "documentation": "^8.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "jest": "^23.1.0",
    "nock": "^9.3.3",
    "node-fetch": "^2.1.2"
  },
  "dependencies": {
    "@8base/apollo-links": "^0.1.3",
    "@8base/sdk": "^0.0.59",
    "apollo-cache-inmemory": "^1.2.4",
    "apollo-client": "^2.3.4",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "ramda": "^0.25.0"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/test/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**",
      "!<rootDir>/**/__tests__/**"
    ]
  },
  "license": "MIT"
}
