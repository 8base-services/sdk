{
  "name": "@8base/auth",
  "version": "0.8.1",
  "main": "dist/index.js",
  "nodule": "dist/index.mjs",
  "scripts": {
    "build": "yarn run build:clean && yarn run build:cjs && yarn run build:mjs && yarn run build:flow",
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "build:js": "babel src --ignore 'src/__fixtures__,**/__tests__'",
    "build:cjs": "yarn run build:js --env-name cjs --out-dir dist/",
    "build:mjs": "yarn run build:js --env-name mjs --out-dir dist/module && for file in $(find dist/module -name '*.js'); do mv \"$file\" `echo \"$file\" | sed 's/dist\\/module/dist/g; s/.js$/.mjs/g'`; done && rm -rf dist/module",
    "build:flow": "yarn flow-copy-source src dist",
    "doc": "NODE_ENV=test documentation readme",
    "docs": "yarn doc src/index.js --section=AuthProvider",
    "test": "NPM_ENV=test jest"
  },
  "peerDependencies": {
    "apollo-client": "^2.4.2",
    "graphql": "0.13.0",
    "graphql-tag": "^2.9.2",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.1",
    "recompose": "0.27.1"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "auth0-js": "^9.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "apollo-client": "^2.4.2",
    "apollo-link": "1.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "documentation": "^8.0.0",
    "flow-copy-source": "^2.0.2",
    "graphql": "0.13.0",
    "graphql-tag": "^2.9.2",
    "jest": "^23.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "recompose": "0.27.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**",
      "!<rootDir>/**/__tests__/**"
    ]
  },
  "license": "MIT"
}
